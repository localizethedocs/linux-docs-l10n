msgstr ""

#: ../../../driver-api/tty/tty_struct:74: include/linux/tty.h:292
msgid "``TTY_HUPPING``"
msgstr ""

#: ../../../driver-api/tty/tty_struct:74: include/linux/tty.h:293
msgid "The TTY is in the process of hanging up to abort potential readers."
msgstr ""

