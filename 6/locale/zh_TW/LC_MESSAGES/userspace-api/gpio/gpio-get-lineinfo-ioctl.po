msgid ""
"This information is available independent of whether the line is in use."
msgstr ""

#: ../../../userspace-api/gpio/gpio-get-lineinfo-ioctl.rst:43
msgid "The line info does not include the line value."
msgstr ""

#: ../../../userspace-api/gpio/gpio-get-lineinfo-ioctl.rst:45
msgid ""
"The line must be requested using gpio-get-linehandle-ioctl.rst or gpio-get-"
"lineevent-ioctl.rst to access its value."
msgstr ""

#: ../../../userspace-api/gpio/gpio-get-lineinfo-ioctl.rst:49
msgid "Return Value"
msgstr ""

#: ../../../userspace-api/gpio/gpio-get-lineinfo-ioctl.rst:51
