#: ../../../networking/seg6-sysctl.rst:11
msgid "seg6_enabled - BOOL"
msgstr ""

#: ../../../networking/seg6-sysctl.rst:12
msgid "Accept or drop SR-enabled IPv6 packets on this interface."
msgstr ""

#: ../../../networking/seg6-sysctl.rst:14
msgid "Relevant packets are those with SRH present and DA = local."
msgstr ""

#: ../../../networking/seg6-sysctl.rst:16
msgid "0 - disabled (default)"
msgstr ""

#: ../../../networking/seg6-sysctl.rst:17
msgid "not 0 - enabled"
msgstr ""

#: ../../../networking/seg6-sysctl.rst:19
msgid "seg6_require_hmac - INTEGER"
msgstr ""

#: ../../../networking/seg6-sysctl.rst:20
msgid "Define HMAC policy for ingress SR-enabled packets on this interface."
msgstr ""

#: ../../../networking/seg6-sysctl.rst:22
msgid "-1 - Ignore HMAC field"
msgstr ""

#: ../../../networking/seg6-sysctl.rst:23
msgid "0 - Accept SR packets without HMAC, validate SR packets with HMAC"
msgstr ""

#: ../../../networking/seg6-sysctl.rst:24
msgid "1 - Drop SR packets without HMAC, validate SR packets with HMAC"
msgstr ""

#: ../../../networking/seg6-sysctl.rst:26
#: ../../../networking/seg6-sysctl.rst:42
msgid "Default is 0."
msgstr ""

