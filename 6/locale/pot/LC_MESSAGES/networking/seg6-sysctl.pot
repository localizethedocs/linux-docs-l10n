msgstr ""

#: ../../../networking/seg6-sysctl.rst:31
msgid "seg6_flowlabel - INTEGER"
msgstr ""

#: ../../../networking/seg6-sysctl.rst:32
msgid ""
"Controls the behaviour of computing the flowlabel of outer IPv6 header in "
"case of SR T.encaps"
msgstr ""

#: ../../../networking/seg6-sysctl.rst:36
msgid "-1"
msgstr ""

#: ../../../networking/seg6-sysctl.rst:36
msgid "set flowlabel to zero."
msgstr ""

#: ../../../networking/seg6-sysctl.rst:37
msgid "0"
msgstr ""

#: ../../../networking/seg6-sysctl.rst:37
msgid ""
"copy flowlabel from Inner packet in case of Inner IPv6 (Set flowlabel to 0 "
"in case IPv4/L2)"
msgstr ""

#: ../../../networking/seg6-sysctl.rst:39
msgid "1"
msgstr ""

#: ../../../networking/seg6-sysctl.rst:39
msgid "Compute the flowlabel using seg6_make_flowlabel()"
msgstr ""
