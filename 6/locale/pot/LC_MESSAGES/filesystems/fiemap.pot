msgid "byte offset of extent on disk"
msgstr ""

#: ../../../filesystems/fiemap:73: include/uapi/linux/fiemap.h:21
msgid "``fe_length``"
msgstr ""

#: ../../../filesystems/fiemap:73: include/uapi/linux/fiemap.h:22
msgid "length in bytes for this extent"
msgstr ""

