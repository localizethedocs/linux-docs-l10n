msgid "ro"
msgstr ""

#: ../../../hwmon/qnap-mcu-hwmon.rst:24
msgid "fan tachometer speed in RPM"
msgstr ""

#: ../../../hwmon/qnap-mcu-hwmon.rst:25
msgid "pwm1"
msgstr ""

