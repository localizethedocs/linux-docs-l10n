msgstr ""

#: ../../../iio/ad7944.rst:127
msgid "Raw ADC voltage value (*IN+* referenced to ground sense)."
msgstr ""

#: ../../../iio/ad7944.rst:129
msgid "``in_voltage0_scale``"
msgstr ""

#: ../../../iio/ad7944.rst:129 ../../../iio/ad7944.rst:145
