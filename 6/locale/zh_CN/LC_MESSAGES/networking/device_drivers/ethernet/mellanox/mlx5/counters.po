msgstr ""

#: ../../../networking/device_drivers/ethernet/mellanox/mlx5/counters.rst:1336
msgid "`outbound_pci_stalled_wr_events`"
msgstr ""

#: ../../../networking/device_drivers/ethernet/mellanox/mlx5/counters.rst:1337
msgid "The number of seconds where `outbound_pci_stalled_wr` was above 30%."
msgstr ""

