#: ../../../userspace-api/media/v4l/pixfmt-cnf4.rst:7
msgid "V4L2_PIX_FMT_CNF4 ('CNF4')"
msgstr ""

#: ../../../userspace-api/media/v4l/pixfmt-cnf4.rst:9
msgid "Depth sensor confidence information as a 4 bits per pixel packed array"
msgstr ""

#: ../../../userspace-api/media/v4l/pixfmt-cnf4.rst:12
msgid "Description"
msgstr ""

#: ../../../userspace-api/media/v4l/pixfmt-cnf4.rst:14
msgid ""
"Proprietary format used by Intel RealSense Depth cameras containing depth "
"confidence information in range 0-15 with 0 indicating that the sensor was "
"unable to resolve any signal and 15 indicating maximum level of confidence "
"for the specific sensor (actual error margins might change from sensor to "
"sensor)."
msgstr ""

#: ../../../userspace-api/media/v4l/pixfmt-cnf4.rst:19
msgid ""
"Every two consecutive pixels are packed into a single byte. Bits 0-3 of byte "
"n refer to confidence value of depth pixel 2*n, bits 4-7 to confidence value "
"of depth pixel 2*n+1."
msgstr ""

#: ../../../userspace-api/media/v4l/pixfmt-cnf4.rst:23
msgid "**Bit-packed representation.**"
msgstr ""

#: ../../../userspace-api/media/v4l/pixfmt-cnf4.rst:30
msgid "Y'\\ :sub:`01[3:0]`\\ (bits 7--4) Y'\\ :sub:`00[3:0]`\\ (bits 3--0)"
msgstr ""

#: ../../../userspace-api/media/v4l/pixfmt-cnf4.rst:31
msgid "Y'\\ :sub:`03[3:0]`\\ (bits 7--4) Y'\\ :sub:`02[3:0]`\\ (bits 3--0)"
msgstr ""
