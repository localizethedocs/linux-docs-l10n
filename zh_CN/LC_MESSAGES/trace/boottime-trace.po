#: ../../../trace/boottime-trace.rst:75
msgid "ftrace.[instance.INSTANCE.]alloc_snapshot"
msgstr ""

#: ../../../trace/boottime-trace.rst:76
msgid "Allocate snapshot buffer."
msgstr ""

#: ../../../trace/boottime-trace.rst:78
msgid "ftrace.[instance.INSTANCE.]cpumask = CPUMASK"
