msgid ""
"Dump ftrace on Oops. If MODE = 1 or omitted, dump trace buffer on all CPUs. "
"If MODE = 2, dump a buffer on a CPU which kicks Oops."
msgstr ""

#: ../../../trace/boottime-trace.rst:43
msgid "kernel.traceoff_on_warning"
msgstr ""

#: ../../../trace/boottime-trace.rst:44
msgid "Stop tracing if WARN_ON() occurs."
msgstr ""

#: ../../../trace/boottime-trace.rst:46
msgid "kernel.fgraph_max_depth = MAX_DEPTH"
msgstr ""

#: ../../../trace/boottime-trace.rst:47
msgid "Set MAX_DEPTH to maximum depth of fgraph tracer."
msgstr ""

#: ../../../trace/boottime-trace.rst:49
msgid "kernel.fgraph_filters = FILTER[, FILTER2...]"
msgstr ""

#: ../../../trace/boottime-trace.rst:50
msgid "Add fgraph tracing function filters."
msgstr ""

#: ../../../trace/boottime-trace.rst:52
msgid "kernel.fgraph_notraces = FILTER[, FILTER2...]"
msgstr ""

#: ../../../trace/boottime-trace.rst:53
msgid "Add fgraph non-tracing function filters."
msgstr ""

#: ../../../trace/boottime-trace.rst:57
msgid "Ftrace Per-instance Options"
msgstr ""

#: ../../../trace/boottime-trace.rst:59
msgid ""
"These options can be used for each instance including global ftrace node."
msgstr ""

#: ../../../trace/boottime-trace.rst:61
msgid "ftrace.[instance.INSTANCE.]options = OPT1[, OPT2[...]]"
msgstr ""

#: ../../../trace/boottime-trace.rst:62
msgid "Enable given ftrace options."
