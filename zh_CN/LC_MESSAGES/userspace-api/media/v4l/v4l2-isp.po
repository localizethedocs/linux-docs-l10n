msgid "The parameters block type (driver-specific)"
msgstr ""

#: ../../../userspace-api/media/v4l/v4l2-isp:67:
#: ../include/uapi/linux/media/v4l2-isp.h:50
msgid "``flags``"
msgstr ""

#: ../../../userspace-api/media/v4l/v4l2-isp:67:
#: ../include/uapi/linux/media/v4l2-isp.h:51
msgid "A bitmask of block flags (driver-specific)"
msgstr ""

