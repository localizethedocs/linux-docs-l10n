#: ../../../userspace-api/media/v4l/ext-ctrls-codec.rst:2535
msgid "``V4L2_CID_MPEG_VIDEO_HEVC_REFRESH_TYPE``"
msgstr ""

#: ../../../userspace-api/media/v4l/ext-ctrls-codec.rst:2538
msgid "enum v4l2_mpeg_video_hevc_hier_refresh_type -"
msgstr ""

#: ../../../userspace-api/media/v4l/ext-ctrls-codec.rst:2539
msgid ""
"Selects refresh type for HEVC encoder. Host has to specify the period into "
"V4L2_CID_MPEG_VIDEO_HEVC_REFRESH_PERIOD."
msgstr ""

#: ../../../userspace-api/media/v4l/ext-ctrls-codec.rst:2553
msgid "``V4L2_MPEG_VIDEO_HEVC_REFRESH_NONE``"
msgstr ""

#: ../../../userspace-api/media/v4l/ext-ctrls-codec.rst:2555
msgid "``V4L2_MPEG_VIDEO_HEVC_REFRESH_CRA``"
msgstr ""

#: ../../../userspace-api/media/v4l/ext-ctrls-codec.rst:2556
msgid "Use CRA (Clean Random Access Unit) picture encoding."
msgstr ""

#: ../../../userspace-api/media/v4l/ext-ctrls-codec.rst:2557
msgid "``V4L2_MPEG_VIDEO_HEVC_REFRESH_IDR``"
msgstr ""

#: ../../../userspace-api/media/v4l/ext-ctrls-codec.rst:2558
msgid "Use IDR (Instantaneous Decoding Refresh) picture encoding."
msgstr ""

#: ../../../userspace-api/media/v4l/ext-ctrls-codec.rst:2565
msgid "``V4L2_CID_MPEG_VIDEO_HEVC_REFRESH_PERIOD (integer)``"
msgstr ""

#: ../../../userspace-api/media/v4l/ext-ctrls-codec.rst:2566
msgid ""
"Selects the refresh period for HEVC encoder. This specifies the number of I "
"pictures between two CRA/IDR pictures. This is valid only if REFRESH_TYPE is "
"not 0."
msgstr ""

#: ../../../userspace-api/media/v4l/ext-ctrls-codec.rst:2570
msgid "``V4L2_CID_MPEG_VIDEO_HEVC_LOSSLESS_CU (boolean)``"
msgstr ""

#: ../../../userspace-api/media/v4l/ext-ctrls-codec.rst:2571
msgid ""
"Indicates HEVC lossless encoding. Setting it to 0 disables lossless "
"encoding. Setting it to 1 enables lossless encoding."
msgstr ""

#: ../../../userspace-api/media/v4l/ext-ctrls-codec.rst:2574
msgid "``V4L2_CID_MPEG_VIDEO_HEVC_CONST_INTRA_PRED (boolean)``"
msgstr ""

#: ../../../userspace-api/media/v4l/ext-ctrls-codec.rst:2575
msgid ""
"Indicates constant intra prediction for HEVC encoder. Specifies the "
"constrained intra prediction in which intra largest coding unit (LCU) "
"prediction is performed by using residual data and decoded samples of "
"neighboring intra LCU only. Setting the value to 1 enables constant intra "
"prediction and setting the value to 0 disables constant intra prediction."
msgstr ""

#: ../../../userspace-api/media/v4l/ext-ctrls-codec.rst:2581
msgid "``V4L2_CID_MPEG_VIDEO_HEVC_WAVEFRONT (boolean)``"
msgstr ""

#: ../../../userspace-api/media/v4l/ext-ctrls-codec.rst:2582
msgid ""
"Indicates wavefront parallel processing for HEVC encoder. Setting it to 0 "
"disables the feature and setting it to 1 enables the wavefront parallel "
"processing."
msgstr ""

#: ../../../userspace-api/media/v4l/ext-ctrls-codec.rst:2586
msgid "``V4L2_CID_MPEG_VIDEO_HEVC_GENERAL_PB (boolean)``"
msgstr ""

#: ../../../userspace-api/media/v4l/ext-ctrls-codec.rst:2587
msgid ""
"Setting the value to 1 enables combination of P and B frame for HEVC encoder."
msgstr ""

#: ../../../userspace-api/media/v4l/ext-ctrls-codec.rst:2590
msgid "``V4L2_CID_MPEG_VIDEO_HEVC_TEMPORAL_ID (boolean)``"
msgstr ""

#: ../../../userspace-api/media/v4l/ext-ctrls-codec.rst:2591
msgid ""
"Indicates temporal identifier for HEVC encoder which is enabled by setting "
"the value to 1."
msgstr ""

#: ../../../userspace-api/media/v4l/ext-ctrls-codec.rst:2594
msgid "``V4L2_CID_MPEG_VIDEO_HEVC_STRONG_SMOOTHING (boolean)``"
msgstr ""

#: ../../../userspace-api/media/v4l/ext-ctrls-codec.rst:2595
msgid ""
"Indicates bi-linear interpolation is conditionally used in the intra "
"prediction filtering process in the CVS when set to 1. Indicates bi-linear "
"interpolation is not used in the CVS when set to 0."
msgstr ""

#: ../../../userspace-api/media/v4l/ext-ctrls-codec.rst:2599
msgid "``V4L2_CID_MPEG_VIDEO_HEVC_MAX_NUM_MERGE_MV_MINUS1 (integer)``"
msgstr ""

#: ../../../userspace-api/media/v4l/ext-ctrls-codec.rst:2600
msgid ""
"Indicates maximum number of merge candidate motion vectors. Values are from "
"0 to 4."
msgstr ""

#: ../../../userspace-api/media/v4l/ext-ctrls-codec.rst:2603
msgid "``V4L2_CID_MPEG_VIDEO_HEVC_TMV_PREDICTION (boolean)``"
msgstr ""

#: ../../../userspace-api/media/v4l/ext-ctrls-codec.rst:2604
msgid ""
"Indicates temporal motion vector prediction for HEVC encoder. Setting it to "
"1 enables the prediction. Setting it to 0 disables the prediction."
msgstr ""

#: ../../../userspace-api/media/v4l/ext-ctrls-codec.rst:2607
msgid "``V4L2_CID_MPEG_VIDEO_HEVC_WITHOUT_STARTCODE (boolean)``"
msgstr ""

#: ../../../userspace-api/media/v4l/ext-ctrls-codec.rst:2608
msgid ""
"Specifies if HEVC generates a stream with a size of the length field instead "
"of start code pattern. The size of the length field is configurable through "
"the V4L2_CID_MPEG_VIDEO_HEVC_SIZE_OF_LENGTH_FIELD control. Setting the value "
"to 0 disables encoding without startcode pattern. Setting the value to 1 "
"will enables encoding without startcode pattern."
msgstr ""

#: ../../../userspace-api/media/v4l/ext-ctrls-codec.rst:2616
msgid "``V4L2_CID_MPEG_VIDEO_HEVC_SIZE_OF_LENGTH_FIELD`` (enum)"
msgstr ""

#: ../../../userspace-api/media/v4l/ext-ctrls-codec.rst:2619
msgid "enum v4l2_mpeg_video_hevc_size_of_length_field -"
msgstr ""

#: ../../../userspace-api/media/v4l/ext-ctrls-codec.rst:2620
msgid ""
"Indicates the size of length field. This is valid when encoding "
"WITHOUT_STARTCODE_ENABLE is enabled."
msgstr ""

#: ../../../userspace-api/media/v4l/ext-ctrls-codec.rst:2633
msgid "``V4L2_MPEG_VIDEO_HEVC_SIZE_0``"
msgstr ""

#: ../../../userspace-api/media/v4l/ext-ctrls-codec.rst:2634
msgid "Generate start code pattern (Normal)."
msgstr ""

#: ../../../userspace-api/media/v4l/ext-ctrls-codec.rst:2635
msgid "``V4L2_MPEG_VIDEO_HEVC_SIZE_1``"
msgstr ""

#: ../../../userspace-api/media/v4l/ext-ctrls-codec.rst:2636
msgid ""
"Generate size of length field instead of start code pattern and length is 1."
msgstr ""

#: ../../../userspace-api/media/v4l/ext-ctrls-codec.rst:2637
msgid "``V4L2_MPEG_VIDEO_HEVC_SIZE_2``"
msgstr ""

#: ../../../userspace-api/media/v4l/ext-ctrls-codec.rst:2638
msgid ""
"Generate size of length field instead of start code pattern and length is 2."
msgstr ""

#: ../../../userspace-api/media/v4l/ext-ctrls-codec.rst:2639
msgid "``V4L2_MPEG_VIDEO_HEVC_SIZE_4``"
msgstr ""

#: ../../../userspace-api/media/v4l/ext-ctrls-codec.rst:2640
msgid ""
"Generate size of length field instead of start code pattern and length is 4."
msgstr ""

#: ../../../userspace-api/media/v4l/ext-ctrls-codec.rst:2646
