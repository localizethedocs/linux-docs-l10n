msgstr ""

#: ../../../userspace-api/media/v4l/pixfmt-sdr-cu16le.rst:10
msgid "Complex unsigned 16-bit little endian IQ sample"
msgstr ""

#: ../../../userspace-api/media/v4l/pixfmt-sdr-cu16le.rst:14
msgid "Description"
msgstr "描述"

#: ../../../userspace-api/media/v4l/pixfmt-sdr-cu16le.rst:16
msgid ""
"This format contains sequence of complex number samples. Each complex number "
"consist two parts, called In-phase and Quadrature (IQ). Both I and Q are "
"represented as a 16 bit unsigned little endian number. I value comes first "
"and Q value after that."
msgstr ""

#: ../../../userspace-api/media/v4l/pixfmt-sdr-cu16le.rst:21
msgid "**Byte Order.** Each cell is one byte."
msgstr ""

#: ../../../userspace-api/media/v4l/pixfmt-sdr-cu16le.rst:29
msgid "start + 0:"
msgstr ""

#: ../../../userspace-api/media/v4l/pixfmt-sdr-cu16le.rst:30
msgid "I'\\ :sub:`0[7:0]`"
msgstr ""

#: ../../../userspace-api/media/v4l/pixfmt-sdr-cu16le.rst:31
msgid "I'\\ :sub:`0[15:8]`"
msgstr ""

#: ../../../userspace-api/media/v4l/pixfmt-sdr-cu16le.rst:32
msgid "start + 2:"
msgstr ""

#: ../../../userspace-api/media/v4l/pixfmt-sdr-cu16le.rst:33
msgid "Q'\\ :sub:`0[7:0]`"
msgstr ""

#: ../../../userspace-api/media/v4l/pixfmt-sdr-cu16le.rst:34
