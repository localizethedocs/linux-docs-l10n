msgid "V4L2_SDR_FMT_CS14LE ('CS14')"
msgstr ""

#: ../../../userspace-api/media/v4l/pixfmt-sdr-cs14le.rst:10
msgid "Complex signed 14-bit little endian IQ sample"
msgstr ""

#: ../../../userspace-api/media/v4l/pixfmt-sdr-cs14le.rst:14
msgid "Description"
msgstr "描述"

#: ../../../userspace-api/media/v4l/pixfmt-sdr-cs14le.rst:16
msgid ""
"This format contains sequence of complex number samples. Each complex number "
"consist two parts, called In-phase and Quadrature (IQ). Both I and Q are "
"represented as a 14 bit signed little endian number. I value comes first and "
"Q value after that. 14 bit value is stored in 16 bit space with unused high "
"bits padded with 0."
msgstr ""

#: ../../../userspace-api/media/v4l/pixfmt-sdr-cs14le.rst:22
msgid "**Byte Order.** Each cell is one byte."
msgstr ""

#: ../../../userspace-api/media/v4l/pixfmt-sdr-cs14le.rst:30
msgid "start + 0:"
msgstr ""

#: ../../../userspace-api/media/v4l/pixfmt-sdr-cs14le.rst:31
msgid "I'\\ :sub:`0[7:0]`"
msgstr ""

#: ../../../userspace-api/media/v4l/pixfmt-sdr-cs14le.rst:32
msgid "I'\\ :sub:`0[13:8]`"
msgstr ""

#: ../../../userspace-api/media/v4l/pixfmt-sdr-cs14le.rst:33
msgid "start + 2:"
msgstr ""

#: ../../../userspace-api/media/v4l/pixfmt-sdr-cs14le.rst:34
