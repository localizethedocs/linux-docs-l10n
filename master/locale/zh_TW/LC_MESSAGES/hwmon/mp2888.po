#: ../../../hwmon/mp2888.rst:19
msgid ""
"This driver implements support for Monolithic Power Systems, Inc. (MPS) "
"vendor dual-loop, digital, multi-phase controller MP2888."
msgstr ""

#: ../../../hwmon/mp2888.rst:22
msgid "This device: supports:"
msgstr ""

#: ../../../hwmon/mp2888.rst:24
msgid "One power rail."
msgstr ""

#: ../../../hwmon/mp2888.rst:25
msgid "Programmable Multi-Phase up to 10 Phases."
msgstr ""

#: ../../../hwmon/mp2888.rst:26
msgid "PWM-VID Interface"
msgstr ""

#: ../../../hwmon/mp2888.rst:27
msgid "One pages 0 for telemetry."
msgstr ""

