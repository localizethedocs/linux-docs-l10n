msgid "fanX_input"
msgstr ""

#: ../../../hwmon/aspeed-pwm-tacho.rst:19
msgid "ro"
msgstr ""

#: ../../../hwmon/aspeed-pwm-tacho.rst:19
msgid ""
"provide current fan rotation value in RPM as reported by the fan to the "
"device."
msgstr ""

#: ../../../hwmon/aspeed-pwm-tacho.rst:22
msgid "pwmX"
msgstr ""

#: ../../../hwmon/aspeed-pwm-tacho.rst:22
msgid "rw"
msgstr ""

#: ../../../hwmon/aspeed-pwm-tacho.rst:22
msgid ""
"get or set PWM fan control value. This is an integer value between 0(off) "
"and 255(full speed)."
msgstr ""
