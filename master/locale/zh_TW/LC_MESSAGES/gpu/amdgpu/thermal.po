#: ../../../gpu/amdgpu/thermal:8: drivers/gpu/drm/amd/pm/amdgpu_pm.c:3217
msgid "hwmon interfaces for GPU temperature:"
msgstr ""

#: ../../../gpu/amdgpu/thermal:8: drivers/gpu/drm/amd/pm/amdgpu_pm.c:3219
msgid ""
"temp[1-3]_input: the on die GPU temperature in millidegrees Celsius - "
"temp2_input and temp3_input are supported on SOC15 dGPUs only"
msgstr ""

#: ../../../gpu/amdgpu/thermal:8: drivers/gpu/drm/amd/pm/amdgpu_pm.c:3222
msgid ""
"temp[1-3]_label: temperature channel label - temp2_label and temp3_label are "
"supported on SOC15 dGPUs only"
msgstr ""

#: ../../../gpu/amdgpu/thermal:8: drivers/gpu/drm/amd/pm/amdgpu_pm.c:3225
msgid ""
"temp[1-3]_crit: temperature critical max value in millidegrees Celsius - "
"temp2_crit and temp3_crit are supported on SOC15 dGPUs only"
msgstr ""

#: ../../../gpu/amdgpu/thermal:8: drivers/gpu/drm/amd/pm/amdgpu_pm.c:3228
msgid ""
"temp[1-3]_crit_hyst: temperature hysteresis for critical limit in "
"millidegrees Celsius - temp2_crit_hyst and temp3_crit_hyst are supported on "
"SOC15 dGPUs only"
msgstr ""

#: ../../../gpu/amdgpu/thermal:8: drivers/gpu/drm/amd/pm/amdgpu_pm.c:3231
msgid ""
"temp[1-3]_emergency: temperature emergency max value(asic shutdown) in "
"millidegrees Celsius - these are supported on SOC15 dGPUs only"
msgstr ""

#: ../../../gpu/amdgpu/thermal:8: drivers/gpu/drm/amd/pm/amdgpu_pm.c:3234
msgid "hwmon interfaces for GPU voltage:"
msgstr ""

#: ../../../gpu/amdgpu/thermal:8: drivers/gpu/drm/amd/pm/amdgpu_pm.c:3236
msgid "in0_input: the voltage on the GPU in millivolts"
msgstr ""

#: ../../../gpu/amdgpu/thermal:8: drivers/gpu/drm/amd/pm/amdgpu_pm.c:3238
msgid "in1_input: the voltage on the Northbridge in millivolts"
msgstr ""

#: ../../../gpu/amdgpu/thermal:8: drivers/gpu/drm/amd/pm/amdgpu_pm.c:3240
msgid "hwmon interfaces for GPU power:"
msgstr ""

#: ../../../gpu/amdgpu/thermal:8: drivers/gpu/drm/amd/pm/amdgpu_pm.c:3242
msgid ""
"power1_average: average power used by the SoC in microWatts.  On APUs this "
"includes the CPU."
msgstr ""

#: ../../../gpu/amdgpu/thermal:8: drivers/gpu/drm/amd/pm/amdgpu_pm.c:3244
msgid ""
"power1_input: instantaneous power used by the SoC in microWatts.  On APUs "
"this includes the CPU."
msgstr ""

#: ../../../gpu/amdgpu/thermal:8: drivers/gpu/drm/amd/pm/amdgpu_pm.c:3246
msgid "power1_cap_min: minimum cap supported in microWatts"
msgstr ""

#: ../../../gpu/amdgpu/thermal:8: drivers/gpu/drm/amd/pm/amdgpu_pm.c:3248
msgid "power1_cap_max: maximum cap supported in microWatts"
msgstr ""

#: ../../../gpu/amdgpu/thermal:8: drivers/gpu/drm/amd/pm/amdgpu_pm.c:3250
msgid "power1_cap: selected power cap in microWatts"
msgstr ""

#: ../../../gpu/amdgpu/thermal:8: drivers/gpu/drm/amd/pm/amdgpu_pm.c:3252
msgid "hwmon interfaces for GPU fan:"
msgstr ""

#: ../../../gpu/amdgpu/thermal:8: drivers/gpu/drm/amd/pm/amdgpu_pm.c:3254
msgid "pwm1: pulse width modulation fan level (0-255)"
msgstr ""

#: ../../../gpu/amdgpu/thermal:8: drivers/gpu/drm/amd/pm/amdgpu_pm.c:3256
msgid ""
"pwm1_enable: pulse width modulation fan control method (0: no fan speed "
"control, 1: manual fan speed control using pwm interface, 2: automatic fan "
"speed control)"
msgstr ""

#: ../../../gpu/amdgpu/thermal:8: drivers/gpu/drm/amd/pm/amdgpu_pm.c:3258
msgid "pwm1_min: pulse width modulation fan control minimum level (0)"
msgstr ""

#: ../../../gpu/amdgpu/thermal:8: drivers/gpu/drm/amd/pm/amdgpu_pm.c:3260
msgid "pwm1_max: pulse width modulation fan control maximum level (255)"
msgstr ""

#: ../../../gpu/amdgpu/thermal:8: drivers/gpu/drm/amd/pm/amdgpu_pm.c:3262
msgid "fan1_min: a minimum value Unit: revolution/min (RPM)"
msgstr ""

#: ../../../gpu/amdgpu/thermal:8: drivers/gpu/drm/amd/pm/amdgpu_pm.c:3264
msgid "fan1_max: a maximum value Unit: revolution/max (RPM)"
msgstr ""

#: ../../../gpu/amdgpu/thermal:8: drivers/gpu/drm/amd/pm/amdgpu_pm.c:3266
msgid "fan1_input: fan speed in RPM"
msgstr ""

#: ../../../gpu/amdgpu/thermal:8: drivers/gpu/drm/amd/pm/amdgpu_pm.c:3268
msgid "fan[1-\\*]_target: Desired fan speed Unit: revolution/min (RPM)"
msgstr ""

#: ../../../gpu/amdgpu/thermal:8: drivers/gpu/drm/amd/pm/amdgpu_pm.c:3270
msgid "fan[1-\\*]_enable: Enable or disable the sensors.1: Enable 0: Disable"
msgstr ""

#: ../../../gpu/amdgpu/thermal:8: drivers/gpu/drm/amd/pm/amdgpu_pm.c:3272
msgid ""
"NOTE: DO NOT set the fan speed via \"pwm1\" and \"fan[1-\\*]_target\" "
"interfaces at the same time."
msgstr ""

#: ../../../gpu/amdgpu/thermal:8: drivers/gpu/drm/amd/pm/amdgpu_pm.c:3273
msgid "That will get the former one overridden."
msgstr ""

#: ../../../gpu/amdgpu/thermal:8: drivers/gpu/drm/amd/pm/amdgpu_pm.c:3275
msgid "hwmon interfaces for GPU clocks:"
msgstr ""

#: ../../../gpu/amdgpu/thermal:8: drivers/gpu/drm/amd/pm/amdgpu_pm.c:3277
msgid "freq1_input: the gfx/compute clock in hertz"
msgstr ""

#: ../../../gpu/amdgpu/thermal:8: drivers/gpu/drm/amd/pm/amdgpu_pm.c:3279
