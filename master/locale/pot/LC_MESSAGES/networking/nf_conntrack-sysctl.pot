msgid "nf_conntrack_log_invalid - INTEGER"
msgstr ""

#: ../../../networking/nf_conntrack-sysctl.rst:84
msgid "0   - disable (default)"
msgstr ""

#: ../../../networking/nf_conntrack-sysctl.rst:85
msgid "1   - log ICMP packets"
msgstr ""

#: ../../../networking/nf_conntrack-sysctl.rst:86
msgid "6   - log TCP packets"
msgstr ""

#: ../../../networking/nf_conntrack-sysctl.rst:87
msgid "17  - log UDP packets"
msgstr ""

