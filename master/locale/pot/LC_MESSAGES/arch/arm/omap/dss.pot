#: ../../../arch/arm/omap/dss.rst:163
msgid "The sysfs interface is divided to two parts: DSS and FB."
msgstr ""

#: ../../../arch/arm/omap/dss.rst:165
msgid ""
"/sys/class/graphics/fb? directory: mirror          0=off, 1=on "
"rotate          Rotation 0-3 for 0, 90, 180, 270 degrees rotate_type     0 = "
"DMA rotation, 1 = VRFB rotation overlays        List of overlay numbers to "
"which framebuffer pixels go phys_addr       Physical address of the "
"framebuffer virt_addr       Virtual address of the framebuffer "
"size            Size of the framebuffer"
msgstr ""

#: ../../../arch/arm/omap/dss.rst:174
msgid ""
"/sys/devices/platform/omapdss/overlay? directory: enabled         0=off, "
"1=on input_size      width,height (ie. the framebuffer size) manager         "
"Destination overlay manager name name output_size     width,height "
"position        x,y screen_width    width global_alpha    global alpha 0-255 "
"0=transparent 255=opaque"
msgstr ""

#: ../../../arch/arm/omap/dss.rst:184
msgid ""
"/sys/devices/platform/omapdss/manager? directory: "
"display                         Destination display name "
"alpha_blending_enabled          0=off, 1=on trans_key_enabled               "
"0=off, 1=on trans_key_type                  gfx-destination, video-source "
"trans_key_value                 transparency color key (RGB24) "
"default_color                   default background color (RGB24)"
msgstr ""

