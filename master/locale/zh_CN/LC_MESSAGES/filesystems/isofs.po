#: ../../../filesystems/isofs.rst:5
msgid "ISO9660 Filesystem"
msgstr ""

#: ../../../filesystems/isofs.rst:7
msgid "Mount options that are the same as for msdos and vfat partitions."
msgstr ""

#: ../../../filesystems/isofs.rst:10
msgid "gid=nnn"
msgstr ""

#: ../../../filesystems/isofs.rst:10
msgid "All files in the partition will be in group nnn."
msgstr ""

#: ../../../filesystems/isofs.rst:11
msgid "uid=nnn"
msgstr ""

#: ../../../filesystems/isofs.rst:11
msgid "All files in the partition will be owned by user id nnn."
msgstr ""

#: ../../../filesystems/isofs.rst:12
msgid "umask=nnn"
msgstr ""

#: ../../../filesystems/isofs.rst:12
msgid "The permission mask (see umask(1)) for the partition."
msgstr ""

#: ../../../filesystems/isofs.rst:15
msgid ""
"Mount options that are the same as vfat partitions. These are only useful "
"when using discs encoded using Microsoft's Joliet extensions."
msgstr ""

#: ../../../filesystems/isofs.rst:19
msgid "iocharset=name"
msgstr ""

#: ../../../filesystems/isofs.rst:19
msgid ""
"Character set to use for converting from Unicode to ASCII.  Joliet filenames "
"are stored in Unicode format, but Unix for the most part doesn't know how to "
"deal with Unicode. There is also an option of doing UTF-8 translations with "
"the utf8 option."
msgstr ""

#: ../../../filesystems/isofs.rst:24
msgid "utf8"
msgstr ""

#: ../../../filesystems/isofs.rst:24
msgid "Encode Unicode names in UTF-8 format. Default is no."
msgstr ""

#: ../../../filesystems/isofs.rst:27
msgid "Mount options unique to the isofs filesystem."
msgstr ""

#: ../../../filesystems/isofs.rst:30
msgid "block=512"
msgstr ""

#: ../../../filesystems/isofs.rst:30
msgid "Set the block size for the disk to 512 bytes"
msgstr ""

#: ../../../filesystems/isofs.rst:31
msgid "block=1024"
msgstr ""

#: ../../../filesystems/isofs.rst:31
msgid "Set the block size for the disk to 1024 bytes"
msgstr ""

#: ../../../filesystems/isofs.rst:32
msgid "block=2048"
msgstr ""

#: ../../../filesystems/isofs.rst:32
msgid "Set the block size for the disk to 2048 bytes"
msgstr ""

#: ../../../filesystems/isofs.rst:33
msgid "check=relaxed"
msgstr ""

#: ../../../filesystems/isofs.rst:33
msgid "Matches filenames with different cases"
msgstr ""

#: ../../../filesystems/isofs.rst:34
msgid "check=strict"
msgstr ""

#: ../../../filesystems/isofs.rst:34
msgid "Matches only filenames with the exact same case"
msgstr ""

#: ../../../filesystems/isofs.rst:35
msgid "cruft"
msgstr ""

#: ../../../filesystems/isofs.rst:35
msgid "Try to handle badly formatted CDs."
msgstr ""

#: ../../../filesystems/isofs.rst:36
msgid "map=off"
msgstr ""

#: ../../../filesystems/isofs.rst:36
msgid "Do not map non-Rock Ridge filenames to lower case"
msgstr ""

#: ../../../filesystems/isofs.rst:37
msgid "map=normal"
msgstr ""

#: ../../../filesystems/isofs.rst:37
msgid "Map non-Rock Ridge filenames to lower case"
msgstr ""

#: ../../../filesystems/isofs.rst:38
msgid "map=acorn"
msgstr ""

#: ../../../filesystems/isofs.rst:38
msgid "As map=normal but also apply Acorn extensions if present"
msgstr ""

#: ../../../filesystems/isofs.rst:39
msgid "mode=xxx"
msgstr ""

#: ../../../filesystems/isofs.rst:39
msgid ""
"Sets the permissions on files to xxx unless Rock Ridge extensions set the "
"permissions otherwise"
msgstr ""

#: ../../../filesystems/isofs.rst:41
msgid "dmode=xxx"
msgstr ""

#: ../../../filesystems/isofs.rst:41
msgid ""
"Sets the permissions on directories to xxx unless Rock Ridge extensions set "
"the permissions otherwise"
msgstr ""

#: ../../../filesystems/isofs.rst:43
msgid "overriderockperm"
msgstr ""

#: ../../../filesystems/isofs.rst:43
msgid ""
"Set permissions on files and directories according to 'mode' and 'dmode' "
"even though Rock Ridge extensions are present."
msgstr ""

#: ../../../filesystems/isofs.rst:46
msgid "nojoliet"
msgstr ""

#: ../../../filesystems/isofs.rst:46
msgid "Ignore Joliet extensions if they are present."
msgstr ""

#: ../../../filesystems/isofs.rst:47
msgid "norock"
msgstr ""

#: ../../../filesystems/isofs.rst:47
msgid "Ignore Rock Ridge extensions if they are present."
msgstr ""

#: ../../../filesystems/isofs.rst:48
msgid "hide"
msgstr ""

#: ../../../filesystems/isofs.rst:48
msgid "Completely strip hidden files from the file system."
msgstr ""

#: ../../../filesystems/isofs.rst:49
msgid "showassoc"
msgstr ""

#: ../../../filesystems/isofs.rst:49
msgid "Show files marked with the 'associated' bit"
msgstr ""

#: ../../../filesystems/isofs.rst:50
msgid "unhide"
msgstr ""

#: ../../../filesystems/isofs.rst:50
msgid ""
"Deprecated; showing hidden files is now default; If given, it is a synonym "
"for 'showassoc' which will recreate previous unhide behavior"
msgstr ""

#: ../../../filesystems/isofs.rst:53
msgid "session=x"
msgstr ""

#: ../../../filesystems/isofs.rst:53
msgid "Select number of session on multisession CD"
msgstr ""

#: ../../../filesystems/isofs.rst:54
msgid "sbsector=xxx"
msgstr ""

#: ../../../filesystems/isofs.rst:54
msgid "Session begins from sector xxx"
msgstr ""

#: ../../../filesystems/isofs.rst:57
msgid "Recommended documents about ISO 9660 standard are located at:"
msgstr ""

#: ../../../filesystems/isofs.rst:59
msgid "http://www.y-adagio.com/"
msgstr ""

#: ../../../filesystems/isofs.rst:60
msgid "ftp://ftp.ecma.ch/ecma-st/Ecma-119.pdf"
msgstr ""

#: ../../../filesystems/isofs.rst:62
msgid ""
"Quoting from the PDF \"This 2nd Edition of Standard ECMA-119 is technically "
"identical with ISO 9660.\", so it is a valid and gratis substitute of the "
"official ISO specification."
msgstr ""
