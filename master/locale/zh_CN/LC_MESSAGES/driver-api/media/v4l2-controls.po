msgstr ""

#: ../../../driver-api/media/v4l2-controls:820: include/media/v4l2-ctrls.h:490
msgid "pointer for storing the minimum value for the control"
msgstr ""

#: ../../../driver-api/media/v4l2-controls:820: include/media/v4l2-ctrls.h:492
