#: ../../../hwmon/ltc2978.rst:240
msgid "Description"
msgstr ""

#: ../../../hwmon/ltc2978.rst:242
msgid "LT7170 and LT7171 are 20 A, 16 V, single- or dual-phase Silent Switcher"
msgstr ""

#: ../../../hwmon/ltc2978.rst:243
msgid "step-down regulators with Digital Power System Management."
msgstr ""

#: ../../../hwmon/ltc2978.rst:244
msgid "LTC2974 and LTC2975 are quad digital power supply managers."
msgstr ""

#: ../../../hwmon/ltc2978.rst:245
msgid "LTC2978 is an octal power supply monitor."
msgstr ""

#: ../../../hwmon/ltc2978.rst:246
msgid "LTC2977 is a pin compatible replacement for LTC2978."
msgstr ""

#: ../../../hwmon/ltc2978.rst:247
msgid "LTC2980 is a 16-channel Power System Manager, consisting of two LTC2977"
msgstr ""

#: ../../../hwmon/ltc2978.rst:248
msgid ""
"in a single die. The chip is instantiated and reported as two separate chips"
msgstr ""

#: ../../../hwmon/ltc2978.rst:249
msgid "on two different I2C bus addresses."
msgstr ""

#: ../../../hwmon/ltc2978.rst:250
msgid ""
"LTC3880, LTC3882, LTC3886, and LTC3887 are dual output poly-phase step-down"
msgstr ""

#: ../../../hwmon/ltc2978.rst:251
msgid "DC/DC controllers."
msgstr ""

#: ../../../hwmon/ltc2978.rst:252
msgid "LTC3883 is a single phase step-down DC/DC controller."
msgstr ""

#: ../../../hwmon/ltc2978.rst:253
msgid "LTM2987 is a 16-channel Power System Manager with two LTC2977 plus"
msgstr ""

#: ../../../hwmon/ltc2978.rst:254
msgid ""
"additional components on a single die. The chip is instantiated and reported"
msgstr ""

#: ../../../hwmon/ltc2978.rst:255
msgid "as two separate chips on two different I2C bus addresses."
msgstr ""

#: ../../../hwmon/ltc2978.rst:256
msgid "LTM4675 is a dual 9A or single 18A Î¼Module regulator"
msgstr ""

#: ../../../hwmon/ltc2978.rst:257
msgid "LTM4676 is a dual 13A or single 26A uModule regulator."
msgstr ""

#: ../../../hwmon/ltc2978.rst:258
msgid "LTM4686 is a dual 10A or single 20A uModule regulator."
msgstr ""

