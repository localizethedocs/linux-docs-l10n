msgstr ""

#: ../../../Documentation/ABI/testing/sysfs-class-bsr:1
msgid "**\\/sys\\/class\\/bsr\\/bsr\\*\\/bsr_size**"
msgstr ""

#: ../../../Documentation/ABI/testing/sysfs-class-bsr:1
msgid ""
