msgid "**\\/sys\\/bus\\/dfl\\/devices\\/dfl_dev\\.X\\/infX_clear**"
msgstr ""

#: ../../../Documentation/ABI/testing/sysfs-bus-dfl-devices-emif:18
msgid ""
"Write-only. Writing \"1\" to this file will zero out all memory data in this "
"memory interface. Writing of other values is invalid. Format: %u"
msgstr ""

#: ../../../Documentation/ABI/testing/sysfs-bus-dfl-devices-emif:9
msgid "**\\/sys\\/bus\\/dfl\\/devices\\/dfl_dev\\.X\\/infX_init_done**"
msgstr ""

#: ../../../Documentation/ABI/testing/sysfs-bus-dfl-devices-emif:9
msgid ""
"Read-only. It indicates if the initialization completed on this memory "
"interface. \"1\" for initialization complete, \"0\" for not yet. Format: %u"
msgstr ""

#: ../../../Documentation/ABI/testing/sysfs-bus-dfl-devices-n3000-nios:41
msgid "**\\/sys\\/bus\\/dfl\\/devices\\/dfl_dev\\.X\\/nios_fw_version**"
msgstr ""

#: ../../../Documentation/ABI/testing/sysfs-bus-dfl-devices-n3000-nios:41
msgid ""
"Read-only. Returns the version of the Nios firmware in the FPGA. Its format "
"is \"major.minor.patch\". Format: %x.%x.%x"
msgstr ""

#: ../../../Documentation/ABI/testing/sysfs-bus-dfl-devices-n3000-nios:13
msgid "**\\/sys\\/bus\\/dfl\\/devices\\/dfl_dev\\.X\\/retimer_A_mode**"
msgstr ""

#: ../../../Documentation/ABI/testing/sysfs-bus-dfl-devices-n3000-nios:13
msgid ""
"Read-only. Returns the enumeration value of the working mode of the retimer "
"A configured by the Nios firmware. The value is read out from shared "
"registers filled by the Nios firmware. Now the values could be:"
msgstr ""

#: ../../../Documentation/ABI/testing/sysfs-bus-dfl-devices-n3000-nios:13
msgid "\"0\": Reset"
msgstr ""

#: ../../../Documentation/ABI/testing/sysfs-bus-dfl-devices-n3000-nios:13
msgid "\"1\": 4x10G"
msgstr ""

#: ../../../Documentation/ABI/testing/sysfs-bus-dfl-devices-n3000-nios:13
msgid "\"2\": 4x25G"
msgstr ""

#: ../../../Documentation/ABI/testing/sysfs-bus-dfl-devices-n3000-nios:13
msgid "\"3\": 2x25G"
msgstr ""

#: ../../../Documentation/ABI/testing/sysfs-bus-dfl-devices-n3000-nios:13
msgid "\"4\": 2x25G+2x10G"
msgstr ""

#: ../../../Documentation/ABI/testing/sysfs-bus-dfl-devices-n3000-nios:13
msgid "\"5\": 1x25G"
msgstr ""

#: ../../../Documentation/ABI/testing/sysfs-bus-dfl-devices-n3000-nios:13
msgid ""
"If the Nios firmware is updated in future to support more retimer modes, "
"more enumeration value is expected. Format: 0x%x"
msgstr ""

#: ../../../Documentation/ABI/testing/sysfs-bus-dfl-devices-n3000-nios:33
msgid "**\\/sys\\/bus\\/dfl\\/devices\\/dfl_dev\\.X\\/retimer_B_mode**"
msgstr ""

#: ../../../Documentation/ABI/testing/sysfs-bus-dfl-devices-n3000-nios:33
msgid ""
"Read-only. Returns the enumeration value of the working mode of the retimer "
"B configured by the Nios firmware. The value format is the same as "
"retimer_A_mode."
msgstr ""

#: ../../../Documentation/ABI/testing/sysfs-bus-dfl:1
msgid "**\\/sys\\/bus\\/dfl\\/devices\\/dfl_dev\\.X\\/type**"
msgstr ""

#: ../../../Documentation/ABI/testing/sysfs-bus-dfl:1
