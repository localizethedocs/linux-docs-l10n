msgid ""
"'status' is a read-only file that returns ASCII text in JSON format "
"reporting the status information."
msgstr ""

#: ../../../Documentation/ABI/testing/sysfs-class-firmware-attributes:455
msgid ""
"\"State\": \"not provisioned | provisioned | provisioning in progress\", "
"\"Version\": \"Major.Minor\", \"Nonce\": <16-bit unsigned number display in "
"base 10>, \"FeaturesInUse\": <16-bit unsigned number display in base 10>, "
"\"EndorsementKeyMod\": \"<256 bytes in base64>\", \"SigningKeyMod\": \"<256 "
"bytes in base64>\""
msgstr ""

