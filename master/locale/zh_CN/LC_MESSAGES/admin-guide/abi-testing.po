msgstr ""

#: ../../../Documentation/ABI/testing/sysfs-class-mtd:125
msgid "**\\/sys\\/class\\/mtd\\/mtdX\\/ecc_strength**"
msgstr ""

#: ../../../Documentation/ABI/testing/sysfs-class-mtd:125
msgid ""
