msgstr ""

#: ../../../Documentation/ABI/testing/sysfs-class-regulator:30
msgid "off"
msgstr ""

#: ../../../Documentation/ABI/testing/sysfs-class-regulator:30
msgid "on"
msgstr ""

#: ../../../Documentation/ABI/testing/sysfs-class-regulator:30
#: ../../../Documentation/ABI/testing/sysfs-firmware-acpi:95
